			<groupId>com.slytechs.jnet.jnetpcap</groupId>
			<artifactId>jnetpcap-wrapper</artifactId>
			<version>${jnetpcap.wrapper.version}</version>
		</dependency>

		<dependency>
			<groupId>com.slytechs.jnet.jnetruntime</groupId>
			<artifactId>jnetruntime-api</artifactId>
			<version>${jnetruntime.version}</version>
		</dependency>

		<dependency>
			<groupId>com.slytechs.jnet.protocol</groupId>
			<artifactId>protocol-pack-api</artifactId>
			<version>${protocol.version}</version>
		</dependency>

